{
  "name": "logseq-plugin-kanban-board",
  "version": "0.1.0",
  "main": "dist/index.html",
  "logseq": {
    "id": "_sethyuan-logseq-kanban-board",
    "icon": "./icon.png"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "mkdir -p dist && cp index-dev.html dist/index.html && preact watch --no-prerender --no-sw",
    "build": "preact build --no-prerender --no-sw --no-inline-css --no-esm --dest dist && rm dist/push-manifest.json"
  },
  "dependencies": {
    "@logseq/libs": "^0.0.14",
    "logseq-l10n": "^0.2.0",
    "preact": "^10.13.0",
    "preact-render-to-string": "^5.2.6",
    "react-beautiful-dnd": "^13.1.1",
    "reactutils": "^5.12.0"
  },
  "devDependencies": {
    "preact-cli": "^3.4.5",
    "preact-cli-svg-loader": "^1.1.0"
  }
}
